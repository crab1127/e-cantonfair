e = imgCover(a.width, a.height);\n        _this.Img.eq(id).find('img')\n          .attr({'width': size.w, 'height': size.h})\n          //.css({'marginLeft': -size.w/2,'marginTop': -size.h/2,});\n        _this.Img.eq(id).fadeIn(600);\n      };\n      a.src = this.data[id].img;\n\n    }\n  };\n    //把图像扩展至足够大，以使图像完全覆盖背景区域。\n    //obj 图片的jq对象\n  function imgCover(imgw, imgh) {\n    var width = document.documentElement.clientWidth,\n        height = document.documentElement.clientHeight;\n    if (width > height) {\n      if (imgw > imgh) {\n        if (width / height > imgw / imgh) {\n          w = width;\n          h = Math.round(imgh * (width / imgw));\n        } else {\n          \n          h = height;\n          w = Math.round(imgw * (height / imgh));\n        }\n      } else {\n        w = width;\n        h = Math.round(imgh * (width / imgw));\n      }\n    } else {\n      if (imgw > imgh) {\n        h = height;\n        w = Math.round(imgw * (height / imgh));\n      } else {\n        if (height / width > imgh / imgw) {\n          h = height;\n          w = Math.round(imgw * (height / imgh));\n        } else {          \n          w = width;\n          h = Math.round(imgh * (width / imgw));\n\n        }\n      }\n    }\n    return {\n      'w':w,\n       'h': h\n    };\n  }\n\n  return AutoPlay;\n});",
